{
    "family": "html-td",
    "executionRoleArn": "arn:aws:iam::456325780689:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "containerDefinitions": [
        {
            "name": "ecs-cl-1",
            "image": "nginx:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "valueCount": 1
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/example-task",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "command": [
                "/bin/sh",
                "-c",
                "echo '<html><body><h1>Hello from my ECS task!</h1></body></html>' > /usr/share/nginx/html/index.html && exec nginx -g 'daemon off;'"
            ]
        }
    ]
}
